{
  "preview": true,
  "name": "codeflea",
  "displayName": "CodeFlea",
  "description": "An extension for intuitively moving the cursor around a code file",
  "publisher": "Richibansoft",
  "repository": {
    "url": "https://github.com/Richiban/CodeFlea/"
  },
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "logo-small.png",
  "keywords": [
    "acejump",
    "cursor",
    "EasyMotion",
    "focus",
    "jump"
  ],
  "activationEvents": [
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:",
    "onCommand:codeFlea.extendBlockSelection",
    "onCommand:codeFlea.extendBlockSelectionBackwards",
    "onCommand:codeFlea.moveToNextBlankLine",
    "onCommand:codeFlea.moveToNextBlock",
    "onCommand:codeFlea.moveToPrevBlock",
    "onCommand:codeFlea.moveToPrevBlankLine",
    "onCommand:codeFlea.moveToNextBlockOfLesserIndentation",
    "onCommand:codeFlea.moveToPrevBlockOfLesserIndentation",
    "onCommand:codeFlea.moveToNextBlockOfSameIndentation",
    "onCommand:codeFlea.moveToPrevBlockOfSameIndentation",
    "onCommand:codeFlea.moveToNextBlockOfGreaterIndentation",
    "onCommand:codeFlea.moveToPrevBlockOfGreaterIndentation",
    "onCommand:codeFlea.nearestLineOfGreaterIndentation",
    "onCommand:codeFlea.nearestLineOfLesserIndentation",
    "onCommand:codeFlea.nextInterestingPoint",
    "onCommand:codeFlea.nextLineOfGreaterIndentation",
    "onCommand:codeFlea.nextLineOfSameIndentation",
    "onCommand:codeFlea.prevInterestingPoint",
    "onCommand:codeFlea.prevLineOfLesserIndentation",
    "onCommand:codeFlea.prevLineOfSameIndentation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "",
      "type": "",
      "properties": {
        "codeFlea.decoration.backgroundColor": {
          "type": "string",
          "default": "lightskyblue",
          "description": "primary decorator background color"
        },
        "codeFlea.decoration.secondaryBackgroundColor": {
          "type": "string",
          "default": "yellow",
          "description": "secondary decorator background color"
        },
        "codeFlea.decoration.backgroundOpacity": {
          "type": "string",
          "default": "0.8"
        },
        "codeFlea.decoration.color": {
          "type": "string",
          "default": "black"
        },
        "codeFlea.decoration.borderColor": {
          "type": "string",
          "default": "black"
        },
        "codeFlea.decoration.width": {
          "type": "number",
          "default": 9
        },
        "codeFlea.decoration.height": {
          "type": "number",
          "default": 13
        },
        "codeFlea.decoration.x": {
          "type": "number",
          "default": 1
        },
        "codeFlea.decoration.y": {
          "type": "number",
          "default": 11
        },
        "codeFlea.decoration.fontSize": {
          "type": "number",
          "default": 12
        },
        "codeFlea.decoration.fontWeight": {
          "type": "string",
          "default": "bold"
        },
        "codeFlea.decoration.fontFamily": {
          "type": "string",
          "default": "Consolas"
        },
        "codeFlea.jump.characters": {
          "type": "string",
          "default": "k, j, d, f, l, s, a, h, g, i, o, n, u, r, v, c, w, e, x, m, b, p, q, t, y, z, K, J, D, F, L, S, A, H, G, I, O, N, U, R, V, C, W, E, X, M, B, P, Q, T, Y, Z"
        },
        "codeFlea.jump.timeout": {
          "type": "number",
          "default": "5",
          "description": "timeout value in seconds to cancel codeFlea jump commands."
        },
        "codeFlea.jump.centerLineAfterJump": {
          "type": "boolean",
          "default": "true"
        }
      }
    },
    "commands": [
      {
        "command": "codeFlea.moveToNextBlock",
        "title": "Move to next block"
      },
      {
        "command": "codeFlea.moveToPrevBlock",
        "title": "Move to previous block"
      },
      {
        "command": "codeFlea.moveToNextBlockOfLesserIndentation",
        "title": "Move to next block of lesser indentation"
      },
      {
        "command": "codeFlea.moveToPrevBlockOfLesserIndentation",
        "title": "Move to previous block of lesser indentation"
      },
      {
        "command": "codeFlea.moveToNextBlockOfSameIndentation",
        "title": "Move to next block of same indentation"
      },
      {
        "command": "codeFlea.moveToPrevBlockOfSameIndentation",
        "title": "Move to previous block of same indentation"
      },
      {
        "command": "codeFlea.moveToNextBlockOfGreaterIndentation",
        "title": "Move to next block of greater indentation"
      },
      {
        "command": "codeFlea.moveToPrevBlockOfGreaterIndentation",
        "title": "Move to previous block of greater indentation"
      },
      {
        "command": "codeFlea.extendBlockSelection",
        "title": "Extend block selection"
      },
      {
        "command": "codeFlea.extendBlockSelectionBackwards",
        "title": "Extend block selection backwards"
      },
      {
        "command": "codeFlea.moveToNextBlankLine",
        "title": "Move to the next blank line"
      },
      {
        "command": "codeFlea.moveToPrevBlankLine",
        "title": "Move to the previous blank line"
      },
      {
        "command": "codeFlea.nearestLineOfLesserIndentation",
        "title": "Go to nearest line of lesser indentation"
      },
      {
        "command": "codeFlea.nearestLineOfGreaterIndentation",
        "title": "Go to nearest line of greater indentation"
      },
      {
        "command": "codeFlea.prevLineOfSameIndentation",
        "title": "Go to the previous line of the same indentation"
      },
      {
        "command": "codeFlea.nextLineOfSameIndentation",
        "title": "Go to the next line of the same indentation"
      },
      {
        "command": "codeFlea.prevInterestingPoint",
        "title": "Go to the previous interesting point"
      },
      {
        "command": "codeFlea.nextInterestingPoint",
        "title": "Go to the next interesting point"
      },
      {
        "command": "codeFlea.jump",
        "title": "Jump to an interesting point"
      }
    ],
    "keybindings": [
      {
        "command": "codeFlea.input.cancel",
        "key": "escape",
        "when": "editorTextFocus && codeFleaInput"
      },
      {
        "key": "ctrl+'",
        "command": "codeFlea.jump"
      },
      {
        "key": "alt+n",
        "command": "codeFlea.moveToNextBlockOfLesserIndentation"
      },
      {
        "key": "alt+l",
        "command": "codeFlea.moveToPrevBlockOfLesserIndentation"
      },
      {
        "key": "alt+e",
        "command": "codeFlea.moveToNextBlockOfSameIndentation"
      },
      {
        "key": "alt+u",
        "command": "codeFlea.moveToPrevBlockOfSameIndentation"
      },
      {
        "key": "alt+shift+e",
        "command": "codeFlea.extendBlockSelection"
      },
      {
        "key": "alt+shift+u",
        "command": "codeFlea.extendBlockSelectionBackwards"
      },
      {
        "key": "alt+i",
        "command": "codeFlea.moveToNextBlockOfGreaterIndentation"
      },
      {
        "key": "alt+y",
        "command": "codeFlea.moveToPrevBlockOfGreaterIndentation"
      },
      {
        "key": "alt+.",
        "command": "codeFlea.moveToNextBlock"
      },
      {
        "key": "alt+m",
        "command": "codeFlea.moveToPrevBlock"
      },
      {
        "key": "alt+o",
        "command": "codeFlea.nextInterestingPoint"
      },
      {
        "key": "alt+h",
        "command": "codeFlea.prevInterestingPoint"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vsce": "^1.81.1",
    "vscode-test": "^1.2.2"
  }
}
