{
  "preview": true,
  "name": "codeflea",
  "displayName": "CodeFlea",
  "description": "An extension for intuitively moving the cursor around a code file",
  "publisher": "Richibansoft",
  "repository": {
    "url": "https://github.com/Richiban/CodeFlea/"
  },
  "version": "0.6.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "logo-small.png",
  "keywords": [
    "acejump",
    "cursor",
    "EasyMotion",
    "focus",
    "jump"
  ],
  "activationEvents": [
    "onCommand:codeFlea.jump",
    "onCommand:codeFlea.extendBlockSelection",
    "onCommand:codeFlea.extendBlockSelectionBackwards",
    "onCommand:codeFlea.nextBlankLine",
    "onCommand:codeFlea.nextBlock",
    "onCommand:codeFlea.prevBlock",
    "onCommand:codeFlea.prevBlankLine",
    "onCommand:codeFlea.nextOuterBlock",
    "onCommand:codeFlea.prevOuterBlock",
    "onCommand:codeFlea.nextSameBlock",
    "onCommand:codeFlea.prevSameBlock",
    "onCommand:codeFlea.nextInnerBlock",
    "onCommand:codeFlea.prevInnerBlock",
    "onCommand:codeFlea.nearestInnerLine",
    "onCommand:codeFlea.nearestOuterLine",
    "onCommand:codeFlea.nextInterestingPoint",
    "onCommand:codeFlea.nextInnerLine",
    "onCommand:codeFlea.nextSameLine",
    "onCommand:codeFlea.prevInterestingPoint",
    "onCommand:codeFlea.prevOuterLine",
    "onCommand:codeFlea.prevSameLine",
    "onCommand:codeFlea.scrollToCursor",
    "onCommand:codeFlea.nextBlockEnd",
    "onCommand:codeFlea.prevBlockEnd",
    "onCommand:codeFlea.scrollEditorUp",
    "onCommand:codeFlea.scrollEditorDown",
    "onCommand:codeFlea.selectAllBlocksInCurrentScope"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "",
      "type": "",
      "properties": {
        "codeFlea.scrollStep": {
          "description": "The number of lines to scroll when scrolling the editor",
          "type": "number",
          "default": "10"
        },
        "codeFlea.decoration.backgroundColor": {
          "type": "string",
          "default": "lightskyblue",
          "description": "primary decorator background color"
        },
        "codeFlea.decoration.secondaryBackgroundColor": {
          "type": "string",
          "default": "yellow",
          "description": "secondary decorator background color"
        },
        "codeFlea.decoration.backgroundOpacity": {
          "type": "string",
          "default": "0.8"
        },
        "codeFlea.decoration.color": {
          "type": "string",
          "default": "black"
        },
        "codeFlea.decoration.borderColor": {
          "type": "string",
          "default": "black"
        },
        "codeFlea.decoration.width": {
          "type": "number",
          "default": 9
        },
        "codeFlea.decoration.height": {
          "type": "number",
          "default": 13
        },
        "codeFlea.decoration.x": {
          "type": "number",
          "default": 1
        },
        "codeFlea.decoration.y": {
          "type": "number",
          "default": 11
        },
        "codeFlea.decoration.fontSize": {
          "type": "number",
          "default": 12
        },
        "codeFlea.decoration.fontWeight": {
          "type": "string",
          "default": "bold"
        },
        "codeFlea.decoration.fontFamily": {
          "type": "string",
          "default": "Consolas"
        },
        "codeFlea.jump.characters": {
          "type": "string",
          "default": "k, j, d, f, l, s, a, h, g, i, o, n, u, r, v, c, w, e, x, m, b, p, q, t, y, z, K, J, D, F, L, S, A, H, G, I, O, N, U, R, V, C, W, E, X, M, B, P, Q, T, Y, Z"
        },
        "codeFlea.jump.timeout": {
          "type": "number",
          "default": "5",
          "description": "timeout value in seconds to cancel codeFlea jump commands."
        },
        "codeFlea.jump.centerLineAfterJump": {
          "type": "boolean",
          "default": "true"
        }
      }
    },
    "commands": [
      {
        "command": "codeFlea.nextBlock",
        "title": "Move to next block"
      },
      {
        "command": "codeFlea.prevBlock",
        "title": "Move to previous block"
      },
      {
        "command": "codeFlea.nextOuterBlock",
        "title": "Move to next block at a higher level"
      },
      {
        "command": "codeFlea.prevOuterBlock",
        "title": "Move to previous block at a higher level"
      },
      {
        "command": "codeFlea.nextSameBlock",
        "title": "Move to next block at the same level"
      },
      {
        "command": "codeFlea.prevSameBlock",
        "title": "Move to previous block at the same level"
      },
      {
        "command": "codeFlea.nextInnerBlock",
        "title": "Move to next block at a lower level"
      },
      {
        "command": "codeFlea.prevInnerBlock",
        "title": "Move to previous block at a lower level"
      },
      {
        "command": "codeFlea.extendBlockSelection",
        "title": "Extend block selection"
      },
      {
        "command": "codeFlea.extendBlockSelectionBackwards",
        "title": "Extend block selection backwards"
      },
      {
        "command": "codeFlea.nextBlankLine",
        "title": "Move to the next blank line"
      },
      {
        "command": "codeFlea.prevBlankLine",
        "title": "Move to the previous blank line"
      },
      {
        "command": "codeFlea.nearestOuterLine",
        "title": "Go to nearest line at a higher level"
      },
      {
        "command": "codeFlea.nearestInnerLine",
        "title": "Go to nearest line at a lower level"
      },
      {
        "command": "codeFlea.prevSameLine",
        "title": "Go to the previous line of the same indentation"
      },
      {
        "command": "codeFlea.nextSameLine",
        "title": "Go to the next line of the same indentation"
      },
      {
        "command": "codeFlea.prevInterestingPoint",
        "title": "Go to the previous interesting point"
      },
      {
        "command": "codeFlea.nextInterestingPoint",
        "title": "Go to the next interesting point"
      },
      {
        "command": "codeFlea.jump",
        "title": "Jump to an interesting point"
      },
      {
        "command": "codeFlea.scrollToCursor",
        "title": "Scroll editor to the cursor"
      },
      {
        "command": "codeFlea.nextBlockEnd",
        "title": "Go to next block end"
      },
      {
        "command": "codeFlea.prevBlockEnd",
        "title": "Go to previous block end"
      },
      {
        "command": "codeFlea.selectAllBlocksInCurrentScope",
        "title": "Select all blocks in current scope"
      },
      {
        "command": "codeFlea.scrollEditorUp",
        "title": "Scroll editor up"
      },
      {
        "command": "codeFlea.scrollEditorDown",
        "title": "Scroll editor down"
      }
    ],
    "keybindings": [
      {
        "command": "codeFlea.input.cancel",
        "key": "escape",
        "when": "editorTextFocus && codeFleaInput"
      },
      {
        "key": "ctrl+'",
        "command": "codeFlea.jump"
      },
      {
        "key": "alt+n",
        "command": "codeFlea.nextOuterBlock"
      },
      {
        "key": "alt+l",
        "command": "codeFlea.prevOuterBlock"
      },
      {
        "key": "alt+e",
        "command": "codeFlea.nextSameBlock"
      },
      {
        "key": "alt+u",
        "command": "codeFlea.prevSameBlock"
      },
      {
        "key": "alt+shift+e",
        "command": "codeFlea.extendBlockSelection"
      },
      {
        "key": "alt+shift+u",
        "command": "codeFlea.extendBlockSelectionBackwards"
      },
      {
        "key": "alt+i",
        "command": "codeFlea.nextInnerBlock"
      },
      {
        "key": "alt+y",
        "command": "codeFlea.prevInnerBlock"
      },
      {
        "key": "alt+.",
        "command": "codeFlea.nextBlock"
      },
      {
        "key": "alt+m",
        "command": "codeFlea.prevBlock"
      },
      {
        "key": "alt+o",
        "command": "codeFlea.nextInterestingPoint"
      },
      {
        "key": "alt+h",
        "command": "codeFlea.prevInterestingPoint"
      },
      {
        "key": "alt+/",
        "command": "codeFlea.scrollToCursor"
      },
      {
        "key": "alt+shift+,",
        "command": "codeFlea.selectAllBlocksInCurrentScope"
      },
      {
        "key": "ctrl+up",
        "command": "codeFlea.scrollEditorUp"
      },
      {
        "key": "ctrl+down",
        "command": "codeFlea.scrollEditorDown"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vsce": "^1.81.1",
    "vscode-test": "^1.2.2"
  }
}
